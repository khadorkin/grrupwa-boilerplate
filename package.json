{
  "name": "grrupwa-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for ultimate stack",
  "main": "index.js",
  "scripts": {
    "build": "babel-node scripts/run build",
    "clean": "babel-node scripts/run clean",
    "copy": "babel-node scripts/run copy",
    "precache": "babel-node scripts/run precache",
    "seed": "babel-node scripts/run seed",
    "start": "npm run update-schema && babel-node scripts/run start",
    "test": "jest",
    "update-schema": "babel-node ./scripts/updateSchema.js"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "isomorphic-style-loader",
      "test/RelayTestUtils"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/test/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "globals": {
      "__DEV__": false
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minheq/grrupwa-boilerplate.git"
  },
  "keywords": [
    "react",
    "relay",
    "graphql",
    "isomorphic",
    "pwa",
    "boilerplate"
  ],
  "author": "Vu Hoang Minh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minheq/grrupwa-boilerplate/issues"
  },
  "homepage": "https://github.com/minheq/grrupwa-boilerplate#readme",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.9.1",
    "bluebird": "^3.4.1",
    "browser-sync": "^2.13.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extend": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs": "^0.8.3",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.27.0",
    "gaze": "^1.1.0",
    "imagemin-webpack-plugin": "^1.0.7",
    "isomorphic-style-loader": "^1.0.0",
    "jest-cli": "^13.0.0",
    "jimp": "^0.2.24",
    "json-loader": "^0.5.4",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "responsive-loader": "^0.5.2",
    "style-loader": "^0.13.1",
    "stylelint": "^6.7.0",
    "stylelint-config-standard": "^9.0.0",
    "sw-precache": "^3.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.13",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.11.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "express-jwt": "^3.4.0",
    "fg-loadcss": "^1.2.0",
    "graphql": "^0.6.0",
    "graphql-relay": "^0.4.2",
    "isomorphic-relay": "^0.7.0",
    "isomorphic-relay-router": "^0.8.0",
    "jsonwebtoken": "^7.0.1",
    "lodash": "^4.13.1",
    "mongoose": "^4.5.2",
    "offline-js": "^0.7.18",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-intl": "^2.1.3",
    "react-relay": "^0.9.0",
    "react-router": "^2.5.0",
    "react-router-relay": "^0.13.3"
  }
}
